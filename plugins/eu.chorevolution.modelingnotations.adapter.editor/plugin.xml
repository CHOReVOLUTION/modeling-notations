<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated adapter -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="eu.chorevolution.modelingnotations.adapter.presentation.AdapterModelWizardID"
            name="%_UI_AdapterModelWizard_label"
            class="eu.chorevolution.modelingnotations.adapter.presentation.AdapterModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/AdapterModelFile.gif">
         <description>%_UI_AdapterModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated adapter -->
      <editor
            id="eu.chorevolution.modelingnotations.adapter.presentation.AdapterEditorID"
            name="%_UI_AdapterEditor_label"
            icon="icons/full/obj16/AdapterModelFile.gif"
            extensions="adapter"
            class="eu.chorevolution.modelingnotations.adapter.presentation.AdapterEditor"
            contributorClass="eu.chorevolution.modelingnotations.adapter.presentation.AdapterActionBarContributor">
      </editor>
   </extension>

</plugin>
